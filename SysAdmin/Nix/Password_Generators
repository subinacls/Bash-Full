### geenrate 32 characts random passwords, does include look alike characters
genranpass32() { a=`< /dev/urandom tr -dc _A-Z-a-z-0-9 | head -c${1:-32}`; echo $a | tee /var/www/streisand/nginx_password; } # Example: genranpass32

### geenrate 8 characts random passwords, does include look alike characters
genranpass8() { a=`< /dev/urandom tr -dc _A-Z-a-z-0-9 | head -c${1:-8}`; echo $a | tee /var/www/streisand/nginx_password; } # Example: genranpass8
